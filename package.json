{
  "name": "wanki",
  "version": "0.1.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p . && copyfiles -e \"./**/*.ts\" --error --up 1 ./src/**/* ./dist",
    "build:live": "nodemon --watch src --ext * --exec \"npm run clean:dist && npm run build || exit 1\"",
    "clean:dist": "rimraf ./dist/",
    "webpack:build:prod": "npx webpack --config webpack.prod.js",
    "webpack:build:dev": "npx webpack --config webpack.dev.js",
    "webpack:live": "npx webpack -w --config webpack.dev.js"
  },
  "dependencies": {
    "hanzi": "^2.1.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.148",
    "@types/node": "^16.4.1",
    "@types/webpack": "^5.28.0",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  }
}
